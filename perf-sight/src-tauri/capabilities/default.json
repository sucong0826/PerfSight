{
  "identifier": "default",
  "description": "Default capability for the main window/webview. Allows receiving metric batches from the backend via Tauri events.",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:event:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/collector",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
